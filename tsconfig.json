{
  "compilerOptions": {
    "lib": [
      "ES2020",
      "ESNext.AsyncIterable",
      "DOM",
      "DOM.Iterable"
    ],
    "target": "ES2019",
    "moduleResolution": "node",
    "outDir": ".",
    "types": [
      "@cloudflare/workers-types",
    ],
    "importHelpers": true,
    "experimentalDecorators": true,
    "strict": true,
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "declaration": true,
    "declarationMap": true,
    "sourceMap": true,
    "inlineSources": true,
  },
  "include": [
    "typings/*.d.ts",
    "src/**/.ts",
    "src/*.ts"
  ],
  "exclude": []
}